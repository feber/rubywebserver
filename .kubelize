templateVersion: v2.2.1
projectName: rubywebserver
services:
- name: web
  port: "4567"
  environments:
  - name: production
    replica: "1"
  - name: sandbox
    replica: "1"
  - name: staging
    replica: "1"
  nodePort: "34567"
  healthz: healthz
  maxMem: "0.5"
  maxCpu: "1"
  minMem: "0.2"
  minCpu: "1"
  limit: false
  timeout: "1"
  delay: "30"
  language: ruby
  consul: envconsul
